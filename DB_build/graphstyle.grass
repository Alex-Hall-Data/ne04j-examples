  text-color-internal: #FFFFFF;
  caption: '{name}';
}

node.Restaurant {
  color: #FF756E;
  border-color: #E06760;
  text-color-internal: #FFFFFF;
  caption: '{name}';
}

node.Staff {
  color: #DE9BF9;
  border-color: #BF85D6;
  text-color-internal: #FFFFFF;
  caption: '{name}';
}

relationship.Feedback {
  caption: '{user_rating}';
}

node.Course {
  color: #FB95AF;
  border-color: #E0849B;
  text-color-internal: #FFFFFF;
  caption: '{course_name}';
}

node.Semester {
  color: #FFD86E;
